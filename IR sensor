int led = 5;
int pin = 7;
int read = HIGH;

void setup() {
  pinMode(led, OUTPUT);
  pinMode(pin,INPUT);
  Serial.begin(9600);
  
}
void loop() {
  read = digitalRead(pin);
  if (read == LOW) {
    Serial.println("obstacle");
    digitalWrite(led, HIGH);
  }else{
    Serial.println("clear");
    digitalWrite(led,LOW);
  }
}
